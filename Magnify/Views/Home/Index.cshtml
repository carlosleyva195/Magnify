@{
    ViewData["Title"] = "Inicio";
}

<div class="body_wrapper">
    <section class="hosting_banner_area">
        <ul class="list-unstyled b_line">
            <li class="wow fadeInUp" data-wow-delay="0.4s"><img src="img/line_01.png" alt=""></li>
            <li class="wow fadeInUp" data-wow-delay="0.7s"><img src="img/line_02.png" alt=""></li>
            <li class="wow fadeInUp" data-wow-delay="0.9s"><img src="img/line_03.png" alt=""></li>
            <li class="wow fadeInUp" data-wow-delay="1.2s"><img src="img/line_04.png" alt=""></li>
